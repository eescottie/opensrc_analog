only_toplevel=true
place=end
value="
*** Save all devices' currents ***
*.options savecurrents

*** Save all voltages	and others ***
.save all	
*.save @m.xm3.msky130_fd_pr__pfet_01v8_lvt[gm]		
*.save @m.xm4.msky130_fd_pr__pfet_01v8_lvt[gm]		
*.save @m.xm7.msky130_fd_pr__nfet_01v8[gm]		

**** Control statements for simulation ****
*   .control <-- start of control statement
*   tran 10n 10u       <-- transient simulation
*   write    <-- write results into a RAW file
*   quit     <-- leave simulation
*   .endc    <-- end of control statement
********************************************
.control
tran 10n 10u
write
quit
.endc			
"}
